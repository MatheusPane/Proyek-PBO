<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="admin.HapusKursiController" spacing="10" alignment="CENTER" style="-fx-padding: 20;">
    <Label text="Hapus Data Kursi" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
    <TextField promptText="Masukkan ID Kursi" fx:id="idKursiField" />
    <Button text="Hapus" onAction="#handleHapus" />
    <Button text="Kembali" onAction="#handleKembali" />
</VBox>
